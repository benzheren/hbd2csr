@font-face
   font-family BrawlerRegular
   font-style normal
   font-weight normal
   src url('Brawler-webfont.eot')
   src url('Brawler-webfont.eot?#iefix') format('embedded-opentype'),
       url('Brawler-webfont.woff') format('woff'),
       url('Brawler-webfont.ttf') format('truetype'),
       url('Brawler-webfont.svg#BrawlerRegular') format('svg')

body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

#scene
  height: 20000px

#cover
  height: 400px
  left: 50%
  margin-left: -480px
  margin-top: -150px
  position: fixed
  top: 50%
  width: 960px
  z-index: 180

#cover-bloc1
  display: block
  position: absolute
  left: 0

  h1
    font 86px 'BrawlerRegular', Arial, sans-serif
    letter-spacing 0
    position: relative
    left: 0

#cover-bloc2
  display: block
  position: absolute
  left: 0
  top: 100px

  h1
    font 86px 'BrawlerRegular', Arial, sans-serif
    position: relative
    left: 0
